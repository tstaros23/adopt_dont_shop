<%= form_with url: "/applications/:id", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :address %>
  <%= form.text_field :address %>
  <%= form.label :city %>
  <%= form.text_field :city %>
  <%= form.label :state %>
  <%= form.text_field :state %>
  <%= form.label :zip_code %>
  <%= form.text_field :zip_code %>
  <%= form.submit 'Submit' %>
<% end %>
